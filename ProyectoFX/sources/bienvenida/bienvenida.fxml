<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="panel" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.gui.bienvenida.BienvenidaController">
   <top>
      <MenuBar prefHeight="25.0" prefWidth="594.0" BorderPane.alignment="CENTER">
         <menus>
            <Menu mnemonicParsing="false" text="Marcadores">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#irAFavoritos" text="Favoritos" />
                  <MenuItem mnemonicParsing="false" text="Destacados" />
               </items></Menu>
            <Menu mnemonicParsing="false" text="Editar">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#irAEditar" text="Insertar" />
                  <MenuItem mnemonicParsing="false" text="Editar" />
                  <MenuItem mnemonicParsing="false" text="Borrar" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Salir">
              <items>
                <MenuItem mnemonicParsing="false" text="Inicio" />
                  <MenuItem mnemonicParsing="false" text="Salir" />
              </items>
            </Menu>
         </menus>
      </MenuBar>
   </top>
</BorderPane>
